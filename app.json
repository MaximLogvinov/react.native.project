{
  "expo": {
    "name": "Witcher task manager",
    "description": "Witcher, all your current tasks here.",
    "slug": "witcher-task-manger",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "icon": "./src/assets/images/witcher-icon.png",
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3d1f18"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.witcherTaskManager"
    }
  }
}